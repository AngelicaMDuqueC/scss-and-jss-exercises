{
  "env": {
    "browser": true,
    "commonjs": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 5
  },
  "rules": {
    "prettier/prettier": "error",
    "indent": ["error", 2],
    "linebreak-style": ["error"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  },
  "plugins": ["prettier"]
}
